  },
  "homepage": "https://github.com/vincent/not-ngrx#readme",
  "dependencies": {
    "jest-cli": "^24.5.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  }
